  <script>
    $(window).load(function(){
      $("#menu-area").sticky({ topSpacing: 0 });
    });
  </script>
  <script type="text/javascript">
    jQuery(document).ready(function($) {
      $('a[rel*=facebox]').facebox({
        loadingImage : '/images/loader.gif',
        closeImage   : '/images/closelabel.png'
      })
    })
  </script>
  <style>
    #menu-area
	{
		z-index: 50;		
		width: 100%;
		height: 72px;
		/**background-color: transparent**/
		background: '#004D6E';
		box-shadow: 0 8px 30px rgba(0,0,0,.2);
	}
	#sticky
	{
		position: relative;
	  	background: #0070A3 url('overlay.png') repeat;
      	font-family: Droid Sans;
      	font-size: 14.5px;
      	line-height: 2em;
      	font-weight: bold;
      	text-align: center;
      	padding: 0 20px;
      	text-shadow: 0 1px 1px rgba(0,0,0,.2);
      	width: auto;
      	box-sizing:border-box;
      	box-shadow: 0px -25px 18px rgba(0,0,0,.2);
	}
	#sticky a
	{
		text-decoration: none;
		color: #fff;
	}
	#sticky a:hover
	{
		color: #fff000;
	}
	#sticky-wrapper
	{
		margin: 0 auto;
		width: auto;
		padding: 0 20px;
	}
  </style>
<%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900" %>
<%= stylesheet_link_tag "default.css" %>
<%= stylesheet_link_tag "fonts.css"%>

<!--[if IE 6]><link href="default_ie6.css" rel="stylesheet" type="text/css" /><![endif]-->

