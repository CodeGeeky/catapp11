<style type="text/css">
	h1 {
		color: #FFF;
	}
</style>

<hr /><br />
<%= form_for inquiry do |f| %>
	<div>
	<%= f.label :name %><br/>
	<%= f.text_field :name %>
	</div>
	<div>
	<%= f.label :email %><br/>
	<%= f.text_field :email %>
	</div>
	<div>
	<%= f.label :message %><br/>
	<%= f.text_area :message, :size => "45x10" %>
	</div>
	<div>
	<%= f.submit "Send" %>
	</div>
<% end %>